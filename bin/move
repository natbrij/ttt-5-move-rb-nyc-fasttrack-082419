#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"

user_input = gets.chomp

def input_to_index (input)
  input.to_i - 1
end

def move (board, position, char = "X")
  board[position] = char
end
